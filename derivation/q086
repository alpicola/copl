|- fun f -> f 0 + f 1 : (int -> int) -> int by T-Fun {
    f : int -> int |- f 0 + f 1 : int by T-Plus {
        f : int -> int |- f 0 : int by T-App {
            f : int -> int |- f : int -> int by T-Var {};
            f : int -> int |- 0 : int by T-Int {}
        };
        f : int -> int |- f 1 : int by T-App {
            f : int -> int |- f : int -> int by T-Var {};
            f : int -> int |- 1 : int by T-Int {}
        }
    }
}
